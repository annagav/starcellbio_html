@defmixin gradient(POS, START_COLOR, END_COLOR, FALLBACK_COLOR) {
  background-color: FALLBACK_COLOR; /* fallback color if gradients are not supported */
  background-image: -webkit-linear-gradient(POS, START_COLOR, END_COLOR);               /* Chrome 10+,Safari 5.1+ */
  /* @alternate */ background-image: -moz-linear-gradient(POS, START_COLOR, END_COLOR); /* FF3.6+ */
  /* @alternate */ background-image: -ms-linear-gradient(POS, START_COLOR, END_COLOR);  /* IE10 */
  /* @alternate */ background-image: -o-linear-gradient(POS, START_COLOR, END_COLOR);   /* Opera 11.10+ */
}


.scb_s_facs_chart_wrapper {
    position: relative;
    width: 400px;
    height:360px;
}

.scb_s_facs_chart_yaxis {
    position: absolute;
    width:360px;
    height:25px;
    -webkit-transform: rotate(-90deg);
    -moz-transform: rotate(-90deg);
    -o-transform: rotate(-90deg);
    -ms-transform: rotate(-90deg);
    transform: rotate(-90deg);
    left:-165px;
    top:140px;
    text-align: center;
    font-family: 'sourcesanspro-regular';
    font-size: 11pt;
}

.scb_s_facs_chart_xaxis {
    position: absolute;
    top:325px;
    width:400px;
    height:25px;
    display: block;
    text-align: center;
    font-family: 'sourcesanspro-regular';
    font-size: 11pt;
}

.scb_s_facs_chart {
	margin:20px;
	margin-left:10px;
    position: absolute;
    width:330px;
    height:305px;
    top:0px;
    left:25px;
}
.scb_s_facs_sample_selected {
    border-radius: 5px;
    background-color: #e0e0e0;
}

.scb_s_facs_progress {
    display: block;
    width:800px;
    height:120px;
    margin-left:25px;
}

.scb_s_facs_progress_rest {
    background: url('../images/western_blot/progress_bg.png');
    display: inline-block;
    padding: 5px;
    padding-top:8px;
