input[type=radio] {
    margin-right: 7px;
	-webkit-appearance: none;
	margin-bottom: 0px;
	height:13px;
	width:12px;
	border: 1px solid #C0C0C0;
	background-color:#D0D0D0;
	border-radius:6px;
}


input[type=radio]:checked{
	margin-right: 7px;
	-webkit-appearance: none;
	margin-bottom: 0px;
	height:13px;
	width:12px;
	border: 1px solid #000000;
	background-color:#80d1ae;
	border-radius:6px;
}


input[type=radio]:disabled{
	opacity:0.5;
}


